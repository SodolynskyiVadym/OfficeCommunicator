<div class="call-container">
	<p class="call-username">@Call.UserName</p>
	<button class="cancel-button" @onclick="() => OnCancel.InvokeAsync(Call)">Cancel</button>
</div>



@code {
	[Parameter]
	public CallInfo Call { get; set; }

	[Parameter]
	public EventCallback<CallInfo> OnCancel { get; set; }
}
